{
  "CONFIG_PATH": "artifact/config",
  "DATA_PATH": "artifact/data",
  "LOG_PATH": "artifact/log",
  "CHECKPOINT_PATH": "artifact/checkpoint",
  "RESULT_PATH": "artifact/result",
  "MODEL_PATH": "artifact/model/",
  "ERROR_PATH": "artifact/error",
  "LIGHT_MODE": false,
  "AUGMENT_MODE": true,
  "L2_MODE": {
    "enabled": true,
    "lambda": 0.0005
  },
  "DROPOUT_MODE": {
    "enabled": true,
    "rate": 0.3
  },
  "OPTIMIZER": {
    "type": "sgd",
    "learning_rate": 0.05,
    "momentum": 0.9
  },
  "SCHEDULE_MODE": {
    "enabled": true,
    "monitor": "val_accuracy",
    "factor": 0.5,
    "patience": 5,
    "min_lr": 1e-05,
    "verbose": 1
  },
  "EARLY_STOP_MODE": {
    "enabled": true,
    "monitor": "val_accuracy",
    "patience": 15,
    "restore_best_weights": true,
    "verbose": 1
  },
  "EPOCHS_COUNT": 150,
  "BATCH_SIZE": 32
}
